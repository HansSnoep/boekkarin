   <text isSpineText="0" applySpotColor="0" areaTextType="content"><![CDATA[<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN" "http://www.w3.org/TR/REC-html40/strict.dtd"><html><head><meta name="qrichtext" content="1" /><style type="text/css">p, li { white-space: pre-wrap; }</style></head><body style=" font-family:'Calibri'; font-size:20pt; font-weight:400; font-style:normal;"><table style="-qt-table-type: root; margin-top:5px; margin-bottom:5px; margin-left:5px; margin-right:5px;"><tr><td style="border: none;"><p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;"><span style=" font-size:14pt; color:#545454;">Hans</span></p><p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;"><span style=" color:#a6a6a6;">Jij leeft vanuit levens principes </span></p><p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;"><span style=" color:#a6a6a6;">die jou voor mij heel aantrekkelijk maken.</span></p><p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;"><span style=" color:#a6a6a6;"> Waarden, die ik zelf ook graag wil </span></p><p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;"><span style=" color:#a6a6a6;">omarmen en van je wil leren.</span></p></td></tr></table></body></html>]]><outline enabled="0" width="1" color="#ff000000"/>
    <textFormat Alignment="ALIGNLEFT" backgroundColor="#00000000" VerticalIndentMargin="0" font="Calibri,20,-1,5,50,0,0,0,0,0" foregroundColor="#ff545454" IndentMargin="5" hasOutline="0"/>
   </text>
